package com.array;

public class ArrayDemo {
	public void Array() {
	
	int a[][]= {{1,2},{3,4}};
	for(int i=0;i<a.length;i++) {
		for(int j=0;j<a.length;j++) {
			System.out.println(a[i][j]+" ");
			System.out.println();
			
		}
	}
		

	}

	public static void main(String[] args) {
		ArrayDemo obj=new ArrayDemo();
	obj.Array();
		
		
		}

	

	

}
